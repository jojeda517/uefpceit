// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ROL {
  id          Int          @id @default(autoincrement())
  rol         String       @unique @db.VarChar(255)
  DETALLE_ROL DETALLEROL[]
}

model DETALLEROL {
  idRolPertenece     Int
  idUsuarioPertenece Int
  ROL                ROL     @relation(fields: [idRolPertenece], references: [id])
  USUARIO            USUARIO @relation(fields: [idUsuarioPertenece], references: [id])

  @@id([idRolPertenece, idUsuarioPertenece])
}

model USUARIO {
  id                 Int          @id @default(autoincrement())
  idPersonaPertenece Int?         @unique
  correo             String       @unique @db.VarChar(255)
  contrasena         String       @db.VarChar(255)
  DETALLE_ROL        DETALLEROL[]
  PERSONA            PERSONA?     @relation(fields: [idPersonaPertenece], references: [id])
}

model PERSONA {
  id                   Int         @id @default(autoincrement())
  idCampusPertenece    Int
  idParroquiaPertenece Int?
  nombre               String      @db.VarChar(50)
  apellido             String      @db.VarChar(50)
  cedula               String      @unique @db.VarChar(10)
  estado               Boolean?
  direccion            String?     @db.VarChar(100)
  fechaNacimiento      DateTime?
  nacionalidad         String?     @db.VarChar(100)
  telefono             String?     @db.VarChar(10)
  sexo                 String?     @db.VarChar(50)
  foto                 String?     @db.VarChar(255)
  campus               CAMPUS      @relation(fields: [idCampusPertenece], references: [id])
  estudiante           ESTUDIANTE? @relation
  docente              DOCENTE?    @relation
  usuario              USUARIO?    @relation
  parroquia            PARROQUIA?  @relation(fields: [idParroquiaPertenece], references: [id])
}

model PROVINCIA {
  id        Int      @id @default(autoincrement())
  provincia String   @unique @db.VarChar(255)
  CANTON    CANTON[]
}

model CANTON {
  id                   Int         @id @default(autoincrement())
  canton               String      @db.VarChar(255)
  idProvinciaPertenece Int
  PROVINCIA            PROVINCIA   @relation(fields: [idProvinciaPertenece], references: [id])
  PARROQUIA            PARROQUIA[]
}

model PARROQUIA {
  id                Int       @id @default(autoincrement())
  parroquia         String    @db.VarChar(255)
  idCantonPertenece Int
  CANTON            CANTON    @relation(fields: [idCantonPertenece], references: [id])
  PERSONA           PERSONA[]
}

model CAMPUS {
  id        Int       @id @default(autoincrement())
  nombre    String    @db.VarChar(255)
  direccion String?   @db.VarChar(255)
  PERSONA   PERSONA[]
}

model ESTADOCIVIL {
  id          Int          @id @default(autoincrement())
  estadoCivil String       @db.VarChar(255)
  ESTUDIANTE  ESTUDIANTE[]
}

model ETNIA {
  id         Int          @id @default(autoincrement())
  etnia      String       @db.VarChar(255)
  ESTUDIANTE ESTUDIANTE[]
}

model DISCAPACIDAD {
  id                   Int                   @id @default(autoincrement())
  tipo                 String                @unique @db.VarChar(50)
  porcentaje           Int?
  DETALLE_DISCAPACIDAD DETALLEDISCAPACIDAD[]
}

model DETALLEDISCAPACIDAD {
  idDiscapacidadPertenece Int
  idEstudiantePertenece   Int
  Discapacidad            DISCAPACIDAD @relation(fields: [idDiscapacidadPertenece], references: [id])
  ESTUDIANTE              ESTUDIANTE   @relation(fields: [idEstudiantePertenece], references: [id])

  @@id([idDiscapacidadPertenece, idEstudiantePertenece])
}

model ESTUDIANTE {
  id                       Int                   @id @default(autoincrement())
  idPersonaPertenece       Int                   @unique
  idEstadoCivilPertenece   Int
  idEtniaPertenece         Int
  idRepresentantePertenece Int
  trabaja                  Boolean
  nombreTrabajo            String?               @db.VarChar(255)
  tieneHijo                Boolean
  rangoEdadHijo            String?               @db.VarChar(50)
  bonoMies                 Boolean
  numeroCarnetDiscapacidad String?               @unique @db.VarChar(50)
  lugarNacimiento          String?               @db.VarChar(100)
  codigoElectricoUnico     String?               @db.VarChar(50)
  observacion              String?               @db.VarChar(255)
  ESTADO_CIVIL             ESTADOCIVIL           @relation(fields: [idEstadoCivilPertenece], references: [id])
  ETNIA                    ETNIA                 @relation(fields: [idEtniaPertenece], references: [id])
  REPRESENTANTE            REPRESENTANTE         @relation(fields: [idRepresentantePertenece], references: [id])
  PERSONA                  PERSONA               @relation(fields: [idPersonaPertenece], references: [id])
  DETALLE_DISCAPACIDAD     DETALLEDISCAPACIDAD[]
  CALIFICACION             CALIFICACION[]
}

model REPRESENTANTE {
  id         Int          @id @default(autoincrement())
  cedula     String       @unique @db.VarChar(10)
  nombre     String       @db.VarChar(50)
  apellido   String       @db.VarChar(50)
  direccion  String?      @db.VarChar(100)
  telefono   String?      @db.VarChar(10)
  ocupacion  String?      @db.VarChar(255)
  ESTUDIANTE ESTUDIANTE[]
}

model DOCENTE {
  id                         Int                          @id @default(autoincrement())
  idPersonaPertenece         Int                          @unique
  tiempoExperiencia          Int?
  PERSONA                    PERSONA                      @relation(fields: [idPersonaPertenece], references: [id])
  DETALLECURSOMATERIADOCENTE DETALLECURSOMATERIADOCENTE[]
  DETALLEDOCENTETITULO       DETALLEDOCENTETITULO[]
  DETALLEDOCENTEEXPERIENCIA  DETALLEDOCENTEEXPERIENCIA[]
}

model DETALLEDOCENTETITULO {
  idDocentePertenece Int
  idTituloPertenece  Int
  DOCENTE            DOCENTE @relation(fields: [idDocentePertenece], references: [id])
  TITULO             TITULO  @relation(fields: [idTituloPertenece], references: [id])

  @@id([idDocentePertenece, idTituloPertenece])
}

model TITULO {
  id                   Int                    @id @default(autoincrement())
  titulo               String                 @unique @db.VarChar(100)
  DETALLEDOCENTETITULO DETALLEDOCENTETITULO[]
}

model DETALLEDOCENTEEXPERIENCIA {
  idDocentePertenece     Int
  idExperienciaPertenece Int
  cargo                  String      @db.VarChar(255)
  DOCENTE                DOCENTE     @relation(fields: [idDocentePertenece], references: [id])
  EXPERIENCIA            EXPERIENCIA @relation(fields: [idExperienciaPertenece], references: [id])

  @@id([idDocentePertenece, idExperienciaPertenece])
}

model EXPERIENCIA {
  id                        Int                         @id @default(autoincrement())
  institucion               String                      @unique @db.VarChar(255)
  DETALLEDOCENTEEXPERIENCIA DETALLEDOCENTEEXPERIENCIA[]
}

model TIPOPERIODO {
  id          Int       @id @default(autoincrement())
  tipoPeriodo String    @unique @db.VarChar(100)
  PERIODO     PERIODO[]
}

model MODALIDAD {
  id        Int    @id @default(autoincrement())
  modalidad String @unique @db.VarChar(100)
}

model PERIODO {
  id                     Int            @id @default(autoincrement())
  idTipoPeriodoPertenece Int
  nombre                 String         @unique @db.VarChar(100)
  descripcion            String?        @db.VarChar(255)
  estado                 Boolean?
  tipoPeriodo            TIPOPERIODO    @relation(fields: [idTipoPeriodoPertenece], references: [id])
  CALIFICACION           CALIFICACION[]
}

model CURSO {
  id                         Int                          @id @default(autoincrement())
  nombre                     String                       @unique @db.VarChar(100)
  descripcion                String?                      @db.VarChar(100)
  DETALLECURSOMATERIADOCENTE DETALLECURSOMATERIADOCENTE[]
  DETALLEESPECIALIDADCURSO   DETALLEESPECIALIDADCURSO[]
  CALIFICACION               CALIFICACION[]
}

model MATERIA {
  id                         Int                          @id @default(autoincrement())
  nombre                     String                       @unique @db.VarChar(100)
  descripcion                String?                      @db.VarChar(100)
  DETALLECURSOMATERIADOCENTE DETALLECURSOMATERIADOCENTE[]
  CALIFICACION               CALIFICACION[]
}

model DETALLECURSOMATERIADOCENTE {
  idCursoPertenece   Int
  idMateriaPertenece Int
  idDocentePertenece Int
  CURSO              CURSO   @relation(fields: [idCursoPertenece], references: [id])
  MATERIA            MATERIA @relation(fields: [idMateriaPertenece], references: [id])
  DOCENTE            DOCENTE @relation(fields: [idDocentePertenece], references: [id])

  @@id([idCursoPertenece, idMateriaPertenece, idDocentePertenece])
}

model DETALLEESPECIALIDADCURSO {
  idEspecialidadPertenece Int
  idCursoPertenece        Int
  ESPECIALIDAD            ESPECIALIDAD @relation(fields: [idEspecialidadPertenece], references: [id])
  CURSO                   CURSO        @relation(fields: [idCursoPertenece], references: [id])

  @@id([idEspecialidadPertenece, idCursoPertenece])
}

model ESPECIALIDAD {
  id                       Int                        @id @default(autoincrement())
  nombre                   String                     @unique @db.VarChar(100)
  descripcion              String?                    @db.VarChar(100)
  DETALLEESPECIALIDADCURSO DETALLEESPECIALIDADCURSO[]
}

model CALIFICACION {
  id                    Int        @id @default(autoincrement())
  idEstudiantePertenece Int
  idCursoPertenece      Int
  idMateriaPertenece    Int
  idPeriodoPertenece    Int
  ESTUDIANTE            ESTUDIANTE @relation(fields: [idEstudiantePertenece], references: [id])
  CURSO                 CURSO      @relation(fields: [idCursoPertenece], references: [id])
  MATERIA               MATERIA    @relation(fields: [idMateriaPertenece], references: [id])
  PERIODO               PERIODO    @relation(fields: [idPeriodoPertenece], references: [id])
  APORTE                APORTE[]
}

model APORTE {
  id                      Int          @id @default(autoincrement())
  idCalificacionPertenece Int
  tipoAporte              String       @db.VarChar(100)
  valor                   Float
  CALIFICACION            CALIFICACION @relation(fields: [idCalificacionPertenece], references: [id])
}
